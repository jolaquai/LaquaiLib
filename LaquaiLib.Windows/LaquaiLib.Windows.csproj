<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0-windows</TargetFramework>
    <LangVersion>preview</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <PackageVersion>1.1.2</PackageVersion>
    <FileVersion>$(PackageVersion)</FileVersion>
    <AssemblyVersion>$(FileVersion)</AssemblyVersion>
    <Version>$(AssemblyVersion)</Version>
    <AnalysisLevel>preview-all</AnalysisLevel>
    <AnalysisMode>all</AnalysisMode>
    <EnablePreviewFeatures>True</EnablePreviewFeatures>
    <UseWPF>True</UseWPF>
    <UseWindowsForms>True</UseWindowsForms>
    <DebugType>embedded</DebugType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);CS0046,CS8600,CS8601,CS8602,CS8603,CS8604,CS8605,CS8606,CS8607,CS8608,CS8609,CS8610,CS8611,CS8612,CS8613,CS8614,CS8615,CS8616,CS8617,CS8618,CS8619,CS8620,CS8621,CS8622,CS8623,CS8624,CS8625,CS8626,CS8627,CS8628,CS8629,CS8630,CS8631,CS8632,CS8633,CS8634,IDE0058</NoWarn>
  </PropertyGroup>
  <PropertyGroup>
    <Title>LaquaiLib.Windows</Title>
    <Authors>Joshua Laquai</Authors>
    <Description>Personal library.</Description>
    <Copyright>(C) Joshua Laquai 20xx</Copyright>
    <PackageDescription>Personal library. Contains functionality specifically for Windows. Adds references to Windows Forms and WPF.</PackageDescription>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <IncludeSource>True</IncludeSource>
    <IncludeSymbols>True</IncludeSymbols>
    <PackageProjectUrl>https://github.com/jolaquai/LaquaiLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jolaquai/LaquaiLib</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageOutputPath>..\..\LaquaiLib\Scripts\pack</PackageOutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CoreAudio" Version="1.40.0" />
    <PackageReference Include="System.Management" Version="9.0.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\LaquaiLib.Core\LaquaiLib.Core.csproj" />
  </ItemGroup>
</Project>